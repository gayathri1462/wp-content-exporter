{
  "name": "wp-content-exporter",
  "version": "0.1.0",
  "description": "Export WordPress CMS data to CSV (Framer-friendly)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "wp-content-exporter": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.test.json",
    "dev": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' src/index.ts",
    "test:local": "node dist/tests/test.js",
    "test": "npm run build && npm run test:local",
    "type-check": "tsc --noEmit",
    "lint": "tsc --strict --noEmit",
    "prepublishOnly": "npm run build && npm run type-check",
    "changeset": "changeset",
    "version": "changeset version",
    "publish-package": "changeset publish"
  },
  "keywords": [
    "wordpress",
    "rest-api",
    "framer",
    "cms",
    "csv",
    "headless",
    "export",
    "data",
    "json2csv"
  ],
  "author": "Gayathri Devi N",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/wp-content-exporter"
  },
  "bugs": {
    "url": "https://github.com/yourusername/wp-content-exporter/issues"
  },
  "homepage": "https://github.com/yourusername/wp-content-exporter#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "json2csv": "^5.0.7"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/json2csv": "^5.0.7",
    "@types/node": "^25.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}

